{
  "name": "@bonjour-les-pixels/component-library",
  "version": "1.0.22",
  "description": "Bonjour les Pixels -- component-library",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "author": "Bonjour les Pixels",
  "license": "MIT",
  "source": "./src/components/index.ts",
  "dependencies": {
    "@storybook/addon-a11y": "^6.3.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/node": "^15.12.4",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "check-peer-dependencies": "^4.0.0",
    "classnames": "^2.3.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint-plugin-react-hooks": "^4.2.0",
    "postcss": "^8.3.5",
    "rc-motion": "^2.4.4",
    "react": "^17.0.2",
    "react-docgen-typescript": "^2.0.0",
    "react-dom": "^17.0.2 ",
    "react-scripts": "4.0.3",
    "react-test-renderer": "^17.0.2",
    "storybook-addon-react-docgen": "^1.2.42",
    "typescript": "^4.3.4",
    "web-vitals": "^2.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:react": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "rollup -c",
    "buildT": "tsc --outDir dist/lib --declarationDir dist/types --declaration true && rollup -c",
    "build-watch": "rollup -c -w",
    "start-playground": "cd playground && npm run start",
    "i-all": "npm i && cd playground && npm i",
    "dev": "npm-run-all --parallel build-watch start-playground",
    "storybook": "NODE_PATH=src start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "prettier:check": "prettier --check src/**/*.ts{,x}",
    "prettier:fix": "prettier --write src/**/*.ts{,x}",
    "eslint:check": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "eslint:fix": "tsc --noEmit && eslint --fix src/**/*.ts{,x}",
    "fixAll": "npm run eslint:fix && npm run prettier:fix",
    "CPD": "npx check-peer-dependencies"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@storybook/addon-actions": "^6.3.0",
    "@storybook/addon-controls": "^6.3.0",
    "@storybook/addon-essentials": "^6.3.0",
    "@storybook/addon-links": "^6.3.0",
    "@storybook/node-logger": "^6.3.0",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.3.0",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "npm-run-all": "^4.1.5",
    "postcss-modules": "^4.1.3",
    "prettier": "^2.3.1",
    "react-docgen": "^5.4.0",
    "rollup": "^2.52.2",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0"
  },
  "peerDependencies": {}
}
